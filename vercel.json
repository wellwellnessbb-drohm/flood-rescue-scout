{
  "version": 2,
  "builds": [
    {
      "src": "api/flood-rescue.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/flood-rescue",
      "methods": ["GET", "POST", "OPTIONS"],
      "dest": "/api/flood-rescue.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}

